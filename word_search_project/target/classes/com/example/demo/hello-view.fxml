<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="650.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
   <ImageView fx:id="back1" fitHeight="650.0" fitWidth="550.0" opacity="0.6" pickOnBounds="true" />
   <Label fx:id="lblDisplay" layoutX="145.0" text="Word Search">
      <font>
         <Font name="Bodoni MT" size="48.0" />
      </font></Label>
   <GridPane fx:id="gdpPlayGrid" gridLinesVisible="true" layoutX="50.0" layoutY="140.0" prefHeight="400.0" prefWidth="450.0">
      <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints vgrow="SOMETIMES" />
      </rowConstraints>
   </GridPane>
   <Label fx:id="lblTurn" alignment="CENTER" layoutX="187.0" layoutY="568.0" prefHeight="17.0" prefWidth="167.0" />
   <Button fx:id="btnStart" disable="true" layoutX="245.0" layoutY="100.0" mnemonicParsing="false" onAction="#start" prefHeight="33.0" prefWidth="50.0" text="Start">
      <font>
         <Font name="Bodoni MT Poster Compressed" size="18.0" />
      </font></Button>
   <Label fx:id="lblResult" alignment="CENTER" layoutX="139.0" layoutY="51.0" prefHeight="17.0" prefWidth="264.0" text="Select a Word Search Theme:" />
   <Label fx:id="wonLabel" alignment="CENTER" layoutX="131.0" layoutY="555.0" prefHeight="21.0" prefWidth="290.0" text="You Won!! Click play again to restart!" textFill="#0f2017" visible="false">
      <font>
         <Font name="Berlin Sans FB" size="18.0" />
      </font></Label>
   <Button fx:id="animalsB" layoutX="274.0" layoutY="68.0" mnemonicParsing="false" onAction="#animals" prefHeight="20.0" prefWidth="74.0" text="Animals" textFill="#ffaa00">
      <font>
         <Font name="Constantia" size="14.0" />
      </font>
   </Button>
   <Button fx:id="flowersB" layoutX="354.0" layoutY="68.0" mnemonicParsing="false" onAction="#flowers" prefWidth="74.0" text="Flowers" textFill="#ff006a">
      <font>
         <Font name="Constantia" size="14.0" />
      </font>
   </Button>
   <Button fx:id="dessertB" layoutX="194.0" layoutY="68.0" mnemonicParsing="false" onAction="#dessert" prefHeight="28.0" prefWidth="74.0" text="Dessert" textFill="#c656fe">
      <font>
         <Font name="Constantia" size="14.0" />
      </font>
   </Button>
   <Button fx:id="vacaB" layoutX="114.0" layoutY="68.0" mnemonicParsing="false" onAction="#vaca" prefHeight="20.0" prefWidth="74.0" text="Vacation" textFill="#32a0ff">
      <font>
         <Font name="Constantia" size="14.0" />
      </font>
   </Button>
   <Button fx:id="again" layoutX="231.0" layoutY="585.0" mnemonicParsing="false" onAction="#playagain" text="Play Again" visible="false">
      <font>
         <Font name="Elephant" size="12.0" />
      </font>
   </Button>
   <ListView fx:id="allwords" layoutX="378.0" layoutY="563.0" prefHeight="82.0" prefWidth="119.0" />
   <Label layoutX="400.0" layoutY="546.0" text="words to find">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
   </Label>
</AnchorPane>
